@model Laja.ViewModels.AssignmentSubmitViewModel


<ul>
    @foreach (var student in Model.Students)
    {
        <li>
            @student.FirstName @student.LastName
            <ul>
                @foreach (var document in Model.Documents)
            {
                if (document.UserId == student.Id)
                {
                <li>@Html.Partial("_ActivityDocList", document)</li>
                 }
             }
            </ul>
        </li>
    }
</ul>
